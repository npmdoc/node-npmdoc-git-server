{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "buildNpmdoc": "git-server",
    "description": "api documentation for [git-server (v0.2.0)](http://qrpike.github.io/NodeJS-Git-Server/host.coffee.html) [![npm package](https://img.shields.io/npm/v/npmdoc-git-server.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-git-server) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-git-server.svg)](https://travis-ci.org/npmdoc/node-npmdoc-git-server)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "git-server": "*",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-git-server",
    "keywords": [
        "documentation",
        "git-server"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_git_server.js",
    "name": "npmdoc-git-server",
    "nameAlias": "npmdoc_git_server",
    "nameOriginal": "npmdoc-git-server",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-git-server"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "(set -e; npm install \"kaizhu256/node-utility2#alpha\"; utility2 shDeployHeroku)",
        "postinstall": "if [ -f npm_scripts.sh ]; then ./npm_scripts.sh postinstall; fi",
        "start": "(set -e; export PORT=${PORT:-8080}; utility2 start test.js)",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "0.0.1"
}
